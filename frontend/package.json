  "private": true,
  "description": "AUTO PRO REPAIRS SALES & SERVICES - Frontend Application",
  "author": "AUTO PRO",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
